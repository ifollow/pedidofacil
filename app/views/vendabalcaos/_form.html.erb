<%= simple_form_for(@vendabalcao) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :total %>
    <%= f.input :dinheiro %>
    <%= f.input :cartao %>
    <%= f.input :cheque %>
    <%= f.input :nota %>
    <%= f.association :estabelecimento %>
    <%= f.association :funcionario %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
